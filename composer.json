{
    "name": "sonatra/ajax-bundle",
    "type": "symfony-bundle",
    "description": "Sonatra Ajax Bundle",
    "keywords": ["AJAX response"],
    "homepage": "http://sonatra.com",
    "license": "MIT",
    "authors": [
        {
            "name": "François Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
